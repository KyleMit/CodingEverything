{
  "json.schemas": [
    {
      "fileMatch": ["manifest.json"],
      "url": "http://json.schemastore.org/web-manifest"
    }
  ],
  "emmet.includeLanguages": {
    "njk": "html"
  },
  "files.associations": {
    "*.html": "njk"
  },
  "vsicons.associations.files": [
    { "icon": "nunjucks", "extensions": ["njk"], "format": "svg" }
  ],
  "markdownlint.config": {
    "default": true,
    "MD012": false,
    "MD013": false,
    "MD025": false,
    "MD033": false,
    "MD034": false
  },
  "cSpell.words": [
  ],
  "search.exclude": {
    "**/code-samples": true
  }
}
